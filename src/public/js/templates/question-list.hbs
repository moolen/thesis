<div id="question-list-view">
	<ul class="question-list col s12 m6">
		<div class="row no-items-placeholder">
			<div class="col s12">
				<div class="card darken-1">
					<div class="card-content">
						<span class="card-title black-text">Noch keine Fragen..</span>
						{{#is config.isAdmin true}}
							<p>
								Klicke auf das Icon unten links um neue Fragen zu erstellen 
							</p>
						{{/is}}

						{{#is config.isAdmin false}}
							<p>
								Bitte warte einen Moment
							</p>
						{{/is}}
						
					</div>
				</div>		
			</div>
		</div>
	</ul>	
</div>
